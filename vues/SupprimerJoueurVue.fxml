<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="170000" prefWidth="70000">

    <children>
        <HBox spacing="15" GridPane.columnIndex="0" GridPane.rowIndex="0">
            <children>
                <Label text="Choisissez un joueur : " />
                <ComboBox fx:id="CBJoueurs" visibleRowCount="5" />
            </children>
        </HBox>

        <HBox spacing="15" GridPane.columnIndex="0" GridPane.rowIndex="4">
            <children>
                <Button fx:id="BSupprimerJoueur" prefWidth="180" prefHeight="20" onAction="#BSupprimerJoueur"
                        text="Supprimer" />
                <Button fx:id="BFermer" prefWidth="180" prefHeight="20" onAction="#BFermer" text="Fermer" />
            </children>
        </HBox>
    </children>
</AnchorPane>
